algoritmo "Exercício 14."

var
   Chico, Juca: real
   c: inteiro
inicio
      chico <- 1.50
      Juca <- 1.10
      enquanto (Chico > Juca) faca
      Chico <- (Chico + 0.02)
      Juca <- (Juca + 0.03)
      C <- C + 1
      fimenquanto
      escreva ("para que juca seja maior que chico foram necessários", C," anos")
fimalgoritmo
