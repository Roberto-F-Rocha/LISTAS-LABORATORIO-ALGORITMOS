algoritmo "Exercício 08."
var
   i :inteiro
inicio
       mudacor ("branco")("fundos")
       mudacor ("azul")("frente")
       escreval ("Todos os numeros pares do 0 ao 100 são:")
       para i <- 0 ate 100 passo 2 faca
       escreval ("")
       escreval (i)
       escreval ("")
       fimpara
fimalgoritmo