algoritmo "Exercício 07."
var
   i :inteiro
inicio
       mudacor ("branco")("fundos")
       mudacor ("azul")("frente")
       escreval ("Todos os numeros ímpares do 0 ao 100 são:")
       para i <- 1 ate 100 passo 2 faca
       escreval ("")
       escreval (i)
       escreval ("")
       fimpara
fimalgoritmo